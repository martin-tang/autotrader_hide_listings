{
  "name": "AutoTrader.CA Hide Sponsored Listings",
  "description": "Hide or highlight sponsored listings on AutoTrader.ca to improve your car searching experience",
  "permissions": [
    "storage"
  ],
  "options_page": "options.html",
  "author": {
    "email": "martinidevelop@gmail.com"
  },
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon16.png"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.autotrader.ca/*"],
      "js": ["autotrader_hide_sponsored_listings_extension.js"]
    }
  ],
  "homepage_url": "https://chromewebstore.google.com/detail/autotraderca-hide-sponsor/bjlfpfclblolnooaiaiejdfpenlkmaba",
  "short_name": "AutoTrader.CA hide sponsored",
  "minimum_chrome_version": "88"
}
